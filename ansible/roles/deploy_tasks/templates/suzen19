#!/bin/bash

ID="$1"

dockerstdout=$(docker exec ${ID} \
  sh -c 'if [ -d /home/suzen/Documents/projects/lab19/using/simple/bash/commands ]; then echo yep; fi')
dockerstdout=$(tr -dc '[[:print:]]' <<< "$dockerstdout")

if [ "$dockerstdout" = "yep" ]; then
  docker exec ${ID} mkdir \
    "/home/suzen/Documents/projects/lab19/using/simple/bash/commands/{{ levels | selectattr('name', 'equalto', 'suzen19') | map(attribute='flag') | join(',') }}"
fi
